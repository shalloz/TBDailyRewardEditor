/* styles/stylesheet.qss */
QMainWindow {
    background-color: #2b2b2b;
    color: #f0f0f0;
}

QPushButton {
    background-color: #444;
    color: #fff;
    padding: 5px;
    border-radius: 4px;
}

QLineEdit, QSpinBox {
    background-color: #333;
    color: #f0f0f0;
    border: 1px solid #666;
    padding: 3px;
}

QListWidget {
    background-color: #3a3a3a;
    color: #ddd;
}
